#include <stdio.h>
int main ()
{
    int num[3][3],r,c;
    
    printf("Enter 9 numbers: ");
    
    for (r=0;r<3;r++)
    for (c=0;c<3;c++)
    scanf("%d",&num[r][c]);
    
    printf("The array is:\n");
    
    for (r=0;r<3;r++)
    {
        for (c=0;c<3;c++)
        printf("%d ",num[r][c]);
        printf("\n");
    }
    
    printf("The transpose of the matrice is:\n");
    
    for (r=0;r<3;r++)
    {
    for (c=0;c<3;c++)
    printf("%d ",num[c][r]);
    printf("\n");
    }
}
