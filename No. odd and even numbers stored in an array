#include <stdio.h>
int main ()
{
    int a[5],i,even,odd;
    printf("Enter your numbers: ");
for (i=0;i<5;++i)
    scanf("%d",&a[i]);
    
for (i=0;i<5;++i)
    if (a[i]%2==0)
        even++;
    else 
        odd++;
        
        
printf("Number of even digits: %d\n",even);
printf("Number of odd digits: %d",odd);
}
