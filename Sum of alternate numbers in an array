#include <stdio.h>
int main ()
{
    int num[6],i;
    int sum1=0;
    int sum2=0;
    printf("Enter 6 numbers: ");
for (i=0;i<6;++i)
    scanf("%d",&num[i]);
    
for (i=0;i<6;i=i+2)
    sum1=sum1+num[i];
    
for (i=1;i<6;i=i+2)
    sum2=sum2+num[i];
    
printf("Sum of numbers stored at odd places: %d\n",sum1);
printf("Sum of numbers stored at even places: %d",sum2);
}
