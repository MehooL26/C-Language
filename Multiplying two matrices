#include <stdio.h>
int main ()
{
    int num1[3][3],num2[3][3],num3[3][3],r,c,k;

    // first array's input taken and printed.

    printf("Enter 9 numbers for first array:\n");
for(r=0;r<3;r++)
for(c=0;c<3;c++)
scanf("%d",&num1[r][c]);

    // second array's input taken.

    printf("Enter 9 numbers for the second array:\n");
for(r=0;r<3;r++)
for(c=0;c<3;c++)
scanf("%d",&num2[r][c]);

    // printing first array

printf("the first array is: \n");
for(r=0;r<3;r++)
    {
    for (c=0;c<3;c++)
        printf("%d ",num1[r][c]);
        printf("\n");
    }

    // printing second array

printf("the second array is:\n ");
for(r=0;r<3;r++)
    {
        for(c=0;c<3;c++)
        printf("%d ",num2[r][c]);
        printf("\n");
    }

    // multiplying the matrices

for(r=0;r<3;r++)
    {
    for(c=0;c<3;c++)
        for(k=0;k<3;k++)
        printf("i[r][c] += a[r][k]* b[k][c]");
        printf("\n");
    }




}
